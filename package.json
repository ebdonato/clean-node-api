{
    "name": "clean-node-api",
    "version": "1.0.0",
    "description": "NodeJs, Typescript, TDD, DDD, Clean Architecture e SOLID",
    "main": "index.js",
    "scripts": {
        "prepare": "husky install",
        "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
        "test:verbose": "jest --passWithNoTests --runInBand",
        "test:unit": "npm test -- --watch -c jest.config.init.js",
        "test:integration": "npm test -- --watch -c jest.config.integration.js",
        "test:staged": "npm test -- --findRelatedTests",
        "test:ci": "npm test -- --coverage",
        "start": "sucrase-node src/main/server.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
